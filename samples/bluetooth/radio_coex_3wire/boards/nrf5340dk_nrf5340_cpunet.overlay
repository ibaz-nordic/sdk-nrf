/ {
  zephyr,user {
    coex-pta-req-gpios =   <&gpio0 5 GPIO_ACTIVE_HIGH>;
    coex-pta-grant-gpios = <&gpio0 7 GPIO_ACTIVE_HIGH>;
  };

  radio_coex: radio_coex_three_wire {
    status = "okay";
    compatible = "sdc-radio-coex-three-wire";
    req-gpios =     <&gpio0 4 GPIO_ACTIVE_HIGH>;
    pri-dir-gpios = <&gpio0 27 GPIO_ACTIVE_HIGH>;
    grant-gpios =   <&gpio0 6 GPIO_ACTIVE_HIGH>;
    type-delay-us = <8>;
    radio-delay-us = <5>;
    /delete-property/ is-rx-active-level;
  };
};

&radio {
  coex = <&radio_coex>;
};
